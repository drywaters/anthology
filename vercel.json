{
  "builds": [
    {
      "src": "cmd/api/main.go",
      "use": "@vercel/go"
    }
  ],
  "buildCommand": "cd web && npm ci && npm run build",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "cmd/api/main.go"
    },
    {
      "src": "/(.*)",
      "dest": "cmd/api/main.go"
    }
  ]
}
